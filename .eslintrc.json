{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "comma-dangle": "off",
    "indent": "off",
    "no-underscore-dangle": "off",
    "no-console": [
      "error",
      {
        "allow": ["warn", "error", "info"]
      }
    ]
  }
}
